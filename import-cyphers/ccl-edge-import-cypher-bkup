
USING PERIODIC COMMIT LOAD CSV WITH HEADERS FROM 'file:///fec/%s/%s-%s.txt' AS line FIELDTERMINATOR '|' 
MATCH (n1:Committee { CMTE_ID:line.CMTE_ID})-[ccl:CANDIDATE_COMMITTEE]->
(n2:Candidate { CAND_ID:line.CAND_ID })
WHERE ccl IS NULL
CREATE (n1)-[:CANDIDATE_COMMITTEE {
	CAND_ID:line.CAND_ID,
	CAND_ELECTION_YR:line.CAND_ELECTION_YR,
	FEC_ELECTION_YR:line.FEC_ELECTION_YR,
	CMTE_ID:line.CMTE_ID,
	CMTE_TP:line.CMTE_TP,
	CMTE_DSGN:line.CMTE_DSGN,
	LINKAGE_ID:toInt(line.LINKAGE_ID)
}]->(n2)