USING PERIODIC COMMIT LOAD CSV WITH HEADERS FROM 'file:///fec/%s/%s-%s.txt' AS line FIELDTERMINATOR '|' 
MERGE (c:Candidate{
	CAND_ID:line.CAND_ID
})

ON CREATE SET 
c.CAND_NAME = line.CAND_NAME, 
c.CAND_PTY_AFFILIATION = line.CAND_PTY_AFFILIATION, 
c.CAND_ELECTION_YR = line.CAND_ELECTION_YR, 
c.CAND_OFFICE = line.CAND_OFFICE, 
c.CAND_OFFICE = line.CAND_OFFICE,
c.CAND_OFFICE_DISTRICT = line.CAND_OFFICE_DISTRICT,
c.CAND_ICI = line.CAND_ICI,
c.CAND_STATUS = line.CAND_STATUS,
c.CAND_PCC = line.CAND_PCC,
c.CAND_ST1 = line.CAND_ST1,
c.CAND_ST2 = line.CAND_ST2,
c.CAND_CITY = line.CAND_CITY,
c.CAND_ST = line.CAND_ST,
c.CAND_ZIP = line.CAND_ZIP