USING PERIODIC COMMIT LOAD CSV WITH HEADERS FROM 'file:///fec/%s/%s-%s.txt' AS line FIELDTERMINATOR '|' 
MERGE (c:Committee {CMTE_ID:line.CMTE_ID})
ON CREATE SET

c.CMTE_ID = line.CMTE_ID,
c.CMTE_NM = line.CMTE_NM,
c.TRES_NM = line.TRES_NM,
c.CMTE_ST1 = line.CMTE_ST1,
c.CMTE_ST2 = line.CMTE_ST2,
c.CMTE_CITY = line.CMTE_CITY,
c.CMTE_ST = line.CMTE_ST,
c.CMTE_ZIP = line.CMTE_ZIP,
c.CMTE_DSGN = line.CMTE_DSGN,
c.CMTE_TP = line.CMTE_TP,
c.CMTE_PTY_AFFILIATION = line.CMTE_PTY_AFFILIATION,
c.CMTE_FILING_FREQ = line.CMTE_FILING_FREQ,
c.ORG_TP = line.ORG_TP,
c.CONNECTED_ORG_NM = line.CONNECTED_ORG_NM,
c.CAND_ID = line.CAND_ID